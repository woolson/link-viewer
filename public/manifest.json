{
  "manifest_version": 2,
  "name": "链接工具 - LinkTools",
  "version": "1.0.0",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "clipboardRead",
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icons/64.png",
    "default_title": "链接工具",
    "default_popup": "index.html"
  }
}
